services:
  backend:
    build:
      context: C:\Users\icoce\Desktop\Zadatak\Backend
      dockerfile: Backend/Dockerfile
      labels:
        com.microsoft.created-by: visual-studio
        com.microsoft.visual-studio.project-name: Backend
      target: base
    container_name: Backend
    entrypoint: tail -f /dev/null
    environment:
      ASPNETCORE_ENVIRONMENT: Development
      ASPNETCORE_LOGGING__CONSOLE__DISABLECOLORS: "true"
      DOTNET_USE_POLLING_FILE_WATCHER: '1'
      NUGET_FALLBACK_PACKAGES: ''
    image: backend:dev
    labels:
      com.microsoft.visualstudio.debuggee.arguments: ' --additionalProbingPath /root/.nuget/packages  "/app/bin/Debug/netcoreapp3.1/Backend.dll"'
      com.microsoft.visualstudio.debuggee.killprogram: /bin/sh -c "if PID=$$(pidof
        dotnet); then kill $$PID; fi"
      com.microsoft.visualstudio.debuggee.program: dotnet
      com.microsoft.visualstudio.debuggee.workingdirectory: /app
    ports:
    - published: 62612
      target: 80
    - target: 80
    tty: true
    volumes:
    - C:\Users\icoce\Desktop\Zadatak\Backend\Backend:/app:rw
    - C:\Users\icoce\vsdbg\vs2017u5:/remote_debugger:rw
    - C:\Users\icoce\AppData\Roaming\ASP.NET\Https:/root/.aspnet/https:ro
    - C:\Users\icoce\AppData\Roaming\Microsoft\UserSecrets:/root/.microsoft/usersecrets:ro
    - C:\Users\icoce\.nuget\packages:/root/.nuget/packages:ro
    - C:\Users\icoce\Desktop\Zadatak\Backend:/src:rw
  db:
    environment:
      ACCEPT_EULA: "Y"
      SA_PASSWORD: Icocelic2808!
    image: mcr.microsoft.com/mssql/server
    ports:
    - published: 62613
      target: 80
version: '3.4'